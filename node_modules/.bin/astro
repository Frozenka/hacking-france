#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Administrator/Documents/HF/visible-velocity/node_modules/.pnpm/astro@4.15.1_@types+node@18.19.47_rollup@4.21.2_typescript@5.5.4/node_modules/astro/node_modules:/mnt/c/Users/Administrator/Documents/HF/visible-velocity/node_modules/.pnpm/astro@4.15.1_@types+node@18.19.47_rollup@4.21.2_typescript@5.5.4/node_modules:/mnt/c/Users/Administrator/Documents/HF/visible-velocity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Administrator/Documents/HF/visible-velocity/node_modules/.pnpm/astro@4.15.1_@types+node@18.19.47_rollup@4.21.2_typescript@5.5.4/node_modules/astro/node_modules:/mnt/c/Users/Administrator/Documents/HF/visible-velocity/node_modules/.pnpm/astro@4.15.1_@types+node@18.19.47_rollup@4.21.2_typescript@5.5.4/node_modules:/mnt/c/Users/Administrator/Documents/HF/visible-velocity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
